{
  "name": "zensla-cli",
  "version": "1.0.8",
  "description": "A CLI tool for managing Slack workspace operations using Playwright.",
  "main": "dist/index.js",
  "scripts": {
    "auth": "tsx src/auth.ts",
    "dev": "tsx src/index.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "keywords": [
    "slack",
    "channel",
    "playwright",
    "automation",
    "cli"
  ],
  "author": "Makoto Ishida <257zs00042@student.zen.ac.jp>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mktoho12/zensla-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mktoho12/zensla-cli/issues"
  },
  "homepage": "https://github.com/mktoho12/zensla-cli#readme",
  "bin": {
    "zensla": "dist/index.js"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "commander": "^13.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "playwright": "^1.51.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.13.14",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.13",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
